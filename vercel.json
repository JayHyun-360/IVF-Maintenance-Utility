{
  "buildCommand": "npm run build:web",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/api/auth/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "/login",
      "permanent": false
    }
  ]
}
