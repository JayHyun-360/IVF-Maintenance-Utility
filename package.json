{
  "name": "ivf-maintenance-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:web": "npm run dev --workspace=ivf-maintenance-web",
    "build": "npm run build:web",
    "build:web": "npm run build --workspace=ivf-maintenance-web",
    "test": "npm run test --workspace=ivf-maintenance-web",
    "lint": "npm run lint --workspace=ivf-maintenance-web"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5"
  }
}
