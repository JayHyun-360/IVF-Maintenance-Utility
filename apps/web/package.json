    "next": "16.1.4",
    "next-auth": "^4.24.13",
    "prisma": "^5.10.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.4.0",
